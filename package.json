{
  "name": "wohlfuehlgesundheit",
  "version": "1.0.0",
  "description": "WohlfÃ¼hlgesundheit - Holistische Darmtherapie Website mit Astro, Tailwind CSS und Zoom-Integration",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >= 21.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "dev:php": "php -S localhost:8000 -t public",
    "dev:full": "echo 'Starting PHP server on port 8000 and Astro dev server on port 4321...' && (php -S localhost:8000 -t public > /dev/null 2>&1 &) && astro dev",
    "start": "npm run dev:full",
    "build": "npm run instagram:fetch && astro build",
    "preview": "astro preview",
    "deploy": "npm run build && ./deploy.sh",
    "astro": "astro",
    "instagram:fetch": "bash scripts/fetch-instagram.sh",
    "check": "npm run check:astro && npm run check:eslint && npm run check:prettier",
    "check:astro": "astro check",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier -w .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@astrojs/sitemap": "^3.6.0",
    "@astrolib/analytics": "^0.6.1",
    "@astrolib/seo": "^1.0.0-beta.8",
