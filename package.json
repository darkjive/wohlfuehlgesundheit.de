    "vanilla-cookieconsent": "^3.1.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/tailwind": "^6.0.2",
    "@eslint/js": "^9.12.0",
    "@iconify-json/tabler": "^1.2.5",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint__js": "^8.42.3",
